<odoo>
    <data noupdate="1">
        <record id="sequence_wr" model="ir.sequence">
           <field name="name">Secuencia de WR</field>
           <field name="code">logistic.wr.sequence</field>
           <field name="active">TRUE</field>
           <field name="prefix">WR</field>
           <field name="padding">6</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="implementation">no_gap</field>
           <field name="company_id"></field>
        </record>

        <record id="sequence_conmer" model="ir.sequence">
           <field name="name">Secuencia de Conmer</field>
           <field name="code">logistic.conmer.sequence</field>
           <field name="active">TRUE</field>
           <field name="prefix">CONMER-%(day)s-%(month)s-%(year)s/</field>
           <field name="padding">0</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="use_date_range">True</field>
           <field name="implementation">no_gap</field>
           <field name="company_id"></field>
        </record>

        <record id="sequence_bundle" model="ir.sequence">
           <field name="name">Secuencia de Bulto</field>
           <field name="code">logistic.bundle.sequence</field>
           <field name="active">TRUE</field>
           <field name="prefix">BU</field>
           <field name="padding">6</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="implementation">no_gap</field>
           <field name="company_id"></field>
        </record>

        <record id="sequence_alert" model="ir.sequence">
           <field name="name">Secuencia de Alerta</field>
           <field name="code">logistic.alert.sequence</field>
           <field name="active">TRUE</field>
           <field name="prefix">AL</field>
           <field name="padding">6</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="implementation">no_gap</field>
           <field name="company_id"></field>
        </record>
    </data>
</odoo>
